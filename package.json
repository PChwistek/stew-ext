    "webpack-cli": "^3.3.4",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "js-search": "^1.4.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "url-loader": "^2.0.0",
    "webext-redux": "^2.1.2"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --config ./webpack.config.prod.js && ./build.sh",
    "lint": "./node_modules/.bin/eslint  --ext .jsx --ext .js ./",
    "lint:fix": "yarn run lint --fix",
    "storybook": "start-storybook",
    "test": "jest",
    "watch": "NODE_ENV=development webpack --config ./webpack.config.dev.js --watch",
    "watch-firefox": "web-ext run --source-dir ./dist"
