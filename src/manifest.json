{
  "name": "Stew - Tab and Workspace Management",
  "short_name": "Stew",
  "description": "Browsers were built for browsing. Make sure your browser works for work.",
  "version": "1.0.9",
  "manifest_version": 2,
  "icons": {
    "144": "assets/logo-144x144.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "identity"
  ],
  "background": {
    "scripts": [
      "background/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "144": "assets/s-letter.png"
    },
    "default_title": "Stew",
    "theme_icons": [{
      "light": "assets/logo-144x144.png",
      "dark": "assets/logo-144x144.png",
      "size": 128
    }]
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+Comma",
        "mac": "Command+Shift+Comma"
      },
      "global": true
    }
  },
  "oauth2": {
    "client_id": "804631623349-jqtj1jappfno7286mkj4tl29i21au8mk.apps.googleusercontent.com",
    "scopes":[""]
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com https://www.googletagmanager.com https://google-analytics.com https://apis.google.com; object-src 'self'"
}