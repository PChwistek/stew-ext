{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "baseUrl": "./",
    "paths": {
      "<rootDir>": ["./src"],
      "Assets/*": ["./src/assets/*"],
      "Background/*": ["./src/containers/background/*"],
      "Common/*": ["./src/containers/common-ui/*"],
      "Popup/*": ["./src/containers/popup/*"],
      "Containers/*": ["./src/containers/*"]
    }
  },
  "exclude": ["node_modules"]
}